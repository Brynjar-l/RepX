        AbstractExercise <|---- SystemExercise


		abstract class AbstractExercise <<abstract>> {
			ExerciseId: UUID
		}
		class SystemExercise <<POJO>> {

		}
		class UserExercise  <<POJO>> {

		}
	}
@enduml